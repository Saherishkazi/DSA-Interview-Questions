class Solution {
    public static ArrayList<Integer> findUnion(int[] a, int[] b) {
        // code here
        HashSet<Integer> set =new HashSet<>();
        ArrayList<Integer> list =new ArrayList<>();
        
        for(int n : a){
            set.add(n);
        }
        for(int n : b){
            set.add(n);
        }
        for(int n : set){
        list.add(n);
            
        }
        return list;
    }
}
