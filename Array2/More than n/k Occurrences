import java.util.*;

class Solution {
  
    public int countOccurence(int[] arr, int k) {
        int n = arr.length;
        HashMap<Integer, Integer> map = new HashMap<>();

        
        for (int i : arr) {
            map.put(i, map.getOrDefault(i, 0) + 1);
        }

        
        int count = 0;
        for (int key : map.keySet()) {
            if (map.get(key) > n / k) {
                count++;
            }
        }

        return count;
    }
}
